<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= tag.name %>      <%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Do you really want to delete this tag?"} %>
    </li>
  <% end %>
</ul>
</br>
<%= link_to"<< Back to Articles List", articles_path %>